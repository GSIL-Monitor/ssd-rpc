public class StringUtil {
    public static boolean  StringsIsBlank(String ... str){
        if(str==null){
            return true;
        }
        int lenth = str.length;
        for(int i=0;i<lenth;i++){
            if(str[i] == null || "".equals(str[i].trim())){
                return true;
            }
        }
        return false;
    }
}
